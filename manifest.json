{
  "manifest_version": 2,
  "name": "GitHub Fix UI",
  "description": "Extension that fixes the latest UI improvements done by GitHub team",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["https://github.com/*", "https://*.github.io/*"],
      "css": ["fix.css"]
    }
  ],
  "permissions": [],
  "homepage_url": "https://github.com/yhnavein/github-fix-ui",
  "author": "Piotr Dabrowski",
  "icons": {
    "16": "icons/gh_16.png",
    "48": "icons/gh_48.png",
    "128": "icons/gh_128.png"
  }
}
